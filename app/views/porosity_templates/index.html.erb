<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All porosities
    </h1>

    <a href="/porosities/new" class="btn btn-block btn-outline-secondary">
      Add a new porosity
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Level
        </th>

        <th>
          Description
        </th>

        <th>
          Position
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @porosities.each do |porosity| %>
      <tr>
        <!-- Display porosity.id start -->
        <td>
          <%= porosity.id %>
        </td>
        <!-- Display porosity.id end -->

        <!-- Display porosity.level start -->
        <td>
          <%= porosity.level %>
        </td>
        <!-- Display porosity.level end -->

        <!-- Display porosity.description start -->
        <td>
          <%= porosity.description %>
        </td>
        <!-- Display porosity.description end -->

        <!-- Display porosity.position start -->
        <td>
          <%= porosity.position %>
        </td>
        <!-- Display porosity.position end -->

        <!-- Display porosity.created_at start -->
        <td>
          <%= time_ago_in_words(porosity.created_at) %> ago
        </td>
        <!-- Display porosity.created_at end -->

        <!-- Display porosity.updated_at start -->
        <td>
          <%= time_ago_in_words(porosity.updated_at) %> ago
        </td>
        <!-- Display porosity.updated_at end -->

        <td>
          <a href="/porosities/<%= porosity.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
